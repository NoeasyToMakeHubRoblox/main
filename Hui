	Content = "Hai Selamat datang di Olekid Versi Gratis",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local SelTab = Window:MakeTab({
	Name = "Selamat Datang",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Hai Welcome untuk Review"
})

local ComTab = Window:MakeTab({
	Name = "Combat warrior",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Oke"
})

